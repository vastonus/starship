# and the node is not catching up.
if [ "$(echo "$json" | jq -r .result.sync_info.catching_up)" != false ]; then
  echo 1>&2 "$RPC_URL is not ready: node is catching up"
  exit 1
fi

echo "$json" | jq -r .result
exit 0
