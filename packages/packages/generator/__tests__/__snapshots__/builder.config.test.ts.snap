    --keyring-backend test \\
    --home $CHAIN_DIR \\
    --fees 100000$DENOM \\
    --gas $GAS \\
    --output json \\
    --gas-adjustment 1.5 $args $NODE_ARGS --yes > /validator.log

  cat /validator.log | jq
}

