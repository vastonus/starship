NUM_VALIDATORS="\${NUM_VALIDATORS:=1}"
NUM_RELAYERS="\${NUM_RELAYERS:=0}"

# check if the binary has genesis subcommand or not, if not, set CHAIN_GENESIS_CMD to empty
