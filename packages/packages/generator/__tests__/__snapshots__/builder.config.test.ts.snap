if [[ $CODE_TAG =~ ^[0-9a-fA-F]{40}$ ]]; then
  echo "Trying to fetch code from commit hash"
  curl -LO $CODE_REPO/archive/$CODE_TAG.zip
  unzip $CODE_TAG.zip
  code_dir=\${CODE_REPO##*/}-\${CODE_TAG}
