  id=$(connection_id)
  if [[ -n "$id" ]]; then
    echo "Found connection id: $id"
    exit 0
  fi
  echo "Failed to get connection id. Sleeping for 2 secs. Tries left $max_tries"
  ((max_tries--))
  sleep 10
done
