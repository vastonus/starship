  echo "Trying to fetch code from tag with 'v' prefix"
  curl -LO $CODE_REPO/archive/refs/tags/$CODE_TAG.zip
  unzip $CODE_TAG.zip
  code_dir=\${CODE_REPO##*/}-\${CODE_TAG#"v"}
