set -eux

function transfer_token() {
  status_code=$(curl --header "Content-Type: application/json" \\
    --request POST --write-out %{http_code} --silent --output /dev/null \\
    --data '{"denom":"'"$DENOM"'","address":"'"$ADDRESS"'"}' \\
    $FAUCET_URL)
  echo $status_code
}

